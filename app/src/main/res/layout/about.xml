<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grad"
    android:textColor="#000000"
    tools:context=".About">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="379dp"
        android:layout_height="389dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/about"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="271dp"
        android:layout_height="44dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="167dp"
        android:text="A Classification of Genetic Disorder through Deep Learning via Android Smartphone"
        android:textAlignment="center"
        android:textSize="13dp"
        android:textColor="#000000"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="488dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="139dp"
        android:layout_marginStart="33dp"
        android:text="Proponents:"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="111dp"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:text="Lazaro, Alliah"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="-141dp"
        android:text="Molon, Cristine Nicole"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView10"
        android:text="Nabong, Rael Beatriz"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="183dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView9"
        android:text="Talde, Czarina Mae"
        android:textSize="15dp"
        android:textColor="#000000"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView8"
        android:textColor="#000000"
        android:text="Torres, Shiena Marie"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignTop="@+id/textView0"
        android:text="Adviser:"
        android:textSize="15dp"
        android:textColor="#000000"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:textColor="#000000"
        android:layout_alignStart="@+id/textView7"
        android:layout_marginBottom="25dp"
        android:text="Engr. Jessica S. Velasco"
        android:textSize="15dp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/thesis_title"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:fontFamily="@font/manifesto"
        android:text="@string/title"
        android:textColor="#020202"
        android:textSize="33sp"
        android:textStyle="bold" />

</RelativeLayout>